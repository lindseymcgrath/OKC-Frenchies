{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/images/visuals/:path*",
      "destination": "/images/visuals/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/((?!api/|images/visuals/).*)",
      "destination": "/index.html"
    }
  ]
}
